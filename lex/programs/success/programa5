class gato {
	private:
		var {
			int edad, peso;		
			char inicial;
		}
	public:
		func int getEdad(){
			var{	
				int x;
			}
			print(x);
			x = edad;
			return edad;
		}
}

class perro{
	public:
		func void woaf(int times){
			print('w', 'o', 'a', 'f');
		}
}

func void sayX(int x){
	print(x);
}

func void printEdad(gato mycat){
	print(mycat.getEdad());
	print(mycat.edad);
}

func int getEdadSquare(gato mycat){
	var {
		int temp;
	}
	temp = mycat.edad * mycat.getEdad();
	return temp;
}

main {
	var {
		gato ravioli;
		int a;
	}
	a = 5;
	while(a < ravioli.edad && a < 100){
		a = a + 123;
		print(a);
	}
