func void sayX(int xx){
	print(xx);
}

func void printEdad(gato mycat){
	print(mycat.getEdad());
}

func int getEdadSquare(gato mycat){
	var {
		int temp;
	}
	temp = mycat.getEdad() * mycat.getEdad();
	return temp;
}

