program fiborec;

func int fibonacci(int n){
    var {
        int retorno;
    }
    if(n == 0 || n == 1){
        retorno = n;
    } else {
        retorno = fibonacci(n-2) + fibonacci(n-1);
    }
    return retorno;
}

main {
    var{
        int n;
    }
    n = 5
    fibo = fibonacci(n);
    print(fibo, '\n');
}
